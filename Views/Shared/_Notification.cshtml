@if (TempData["msg"] != null)
{

    @if (TempData["msg"] == "User has been registered successfully" || TempData["msg"] == "Logged in Successfully")
    {
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
        <script type="text/javascript">
            toastr.success('@TempData["msg"]');
        </script>

    }
    @if (TempData["msg"] == "User creation failed" || TempData["msg"] == "Error on logging in" || TempData["msg"] == "User locked out" || TempData["msg"] == "Invalid Employee-ID" || TempData["msg"] == "Invalid Password")
    {
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
        <script type="text/javascript">
            toastr.error('@TempData["msg"]');
        </script>

    }
    @if (TempData["msg"] != "User already exists")
    {
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
        <script type="text/javascript">
            toastr.warning('@TempData["msg"]');
        </script>
    }

}